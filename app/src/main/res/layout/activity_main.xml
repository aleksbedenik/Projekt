<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/mainActivity_login_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/mainActivity_layout_textView_MarginLeft"
        android:layout_marginLeft="@dimen/mainActivity_layout_textView_MarginLeft"
        android:layout_marginTop="60dp"
        android:layout_marginEnd="@dimen/mainActivity_layout_textView_MarginRight"
        android:layout_marginRight="@dimen/mainActivity_layout_textView_MarginRight"
        android:text="@string/main_activity_login_text"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/mainActivity_login_text2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/mainActivity_layout_textView_MarginLeft"
        android:layout_marginLeft="@dimen/mainActivity_layout_textView_MarginLeft"
        android:layout_marginTop="36dp"
        android:layout_marginEnd="@dimen/mainActivity_layout_textView_MarginRight"
        android:layout_marginRight="@dimen/mainActivity_layout_textView_MarginRight"
        android:text="Registriraj se"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button" />

    <TextView
        android:id="@+id/mainActivity_login_text4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/mainActivity_layout_textView_MarginLeft"
        android:layout_marginLeft="@dimen/mainActivity_layout_textView_MarginLeft"
        android:layout_marginEnd="@dimen/mainActivity_layout_textView_MarginRight"
        android:layout_marginRight="@dimen/mainActivity_layout_textView_MarginRight"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.243"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName" />

    <TextView
        android:id="@+id/mainActivity_login_text5"
        android:layout_width="3dp"
        android:layout_height="15dp"
        android:layout_marginStart="@dimen/mainActivity_layout_textView_MarginLeft"
        android:layout_marginLeft="@dimen/mainActivity_layout_textView_MarginLeft"
        android:layout_marginTop="52dp"
        android:layout_marginEnd="@dimen/mainActivity_layout_textView_MarginRight"
        android:layout_marginRight="@dimen/mainActivity_layout_textView_MarginRight"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.245"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/mainActivity_login_text" />

    <TextView
        android:id="@+id/mainActivity_login_text3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/mainActivity_layout_textView_MarginLeft"
        android:layout_marginLeft="@dimen/mainActivity_layout_textView_MarginLeft"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="@dimen/mainActivity_layout_textView_MarginRight"
        android:layout_marginRight="@dimen/mainActivity_layout_textView_MarginRight"
        android:text="Pozabil geslo?"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/mainActivity_login_text2"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/button"

        style="@style/AppTheme.Button"
        android:layout_marginTop="24dp"
        android:text="@string/main_activity_login_button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName2" />

    <EditText
        android:id="@+id/editTextTextPersonName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="UporabniÅ¡ko ime"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/mainActivity_login_text5" />

    <EditText
        android:id="@+id/editTextTextPersonName2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Geslo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/mainActivity_login_text4" />

</androidx.constraintlayout.widget.ConstraintLayout>